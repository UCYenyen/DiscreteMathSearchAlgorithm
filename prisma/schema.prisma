generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

enum Algorithm {
  LINEAR_SEARCH
  BINARY_SEARCH
  INTERPOLATION_SEARCH
}

enum Datatype {
  UNIFORM
  NONUNIFORM
  UNSORTED
}

enum DatasetSize {
  SIZE_1K
  SIZE_5K
  SIZE_10K
}

enum FieldToSearch {
  NAMA
  NIM
  JURUSAN
  ALAMAT
}

model AnalysisResult {
  id         String @id @default(cuid())
  iterations Int
  itemsFound Int    @default(0)

  executionTimeMs Float
  startSearchAt   DateTime @default(now())
  endSearchAt     DateTime

  datatype  Datatype
  algorithm Algorithm
  datasetSize DatasetSize
  
  fieldToSearch FieldToSearch
  searchValue   String
  refrenceDataset String
}

model Mahasiswa {
  id      String @id @default(cuid())
  nama    String
  nim     String
  jurusan String
  alamat  String
  size DatasetSize @default(SIZE_1K)
}

